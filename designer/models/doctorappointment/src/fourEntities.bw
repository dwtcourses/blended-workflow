Specification: "Four Entities" 
        
Entity A mandatory {
	att1 : Number 
} 

Entity B {
	att1 : Boolean
}   

Entity C {
	att1 : Boolean
}   

Entity D {
	att1 : Boolean
}   

Association AHasBs {
	A with a (1) 
	B with b (3..3)
}

Association AHasCs {
	A with a (1) 
	C with c (0..*)
}

Association BandC {
	B with b (0..*) 
	C with c (0..*)
}
