    <goal name="Data.bloodPressure" parent="Data">
      <preConditions />
      <postConditions>
        <condition path="Data.bloodPressure" />
      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Data.physicalCondition" parent="Data">
      <preConditions />
      <postConditions>
        <condition path="Data.physicalCondition" />
      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Data" parent="Episode">
      <preConditions />
      <postConditions>
        <condition path="Data" />
      </postConditions>
      <mulConditions>
        <condition name="EpisodeHasData" rolename="episode" cardinality="1" />
      </mulConditions>
    </goal>
    <goal name="Report.desc" parent="Report">
      <preConditions />
      <postConditions>
        <condition path="Report.desc" />
      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Report" parent="Episode">
      <preConditions>
        <condition path="Report.episode.data.bloodPressure" />
      </preConditions>
      <postConditions>
        <condition path="Report" />
      </postConditions>
      <mulConditions>
        <condition name="EpisodehasReport" rolename="episode" cardinality="1" />
      </mulConditions>
    </goal>
    <goal name="Patient.name" parent="Patient">
      <preConditions />
      <postConditions>
        <condition path="Patient.name" />
      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Patient.age" parent="Patient">
      <preConditions />
      <postConditions>
        <condition path="Patient.age" />
      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Patient.heartProblems" parent="Patient">
      <preConditions />
      <postConditions>
        <condition path="Patient.heartProblems" />
      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Patient" parent="Episode">
      <preConditions />
      <postConditions>
        <condition path="Patient" />
      </postConditions>
      <mulConditions>
        <condition name="PatientHasEpisode" rolename="episode" cardinality="0..*" />
      </mulConditions>
    </goal>
    <goal name="Doctor.name" parent="Doctor">
      <preConditions />
      <postConditions>
        <condition path="Doctor.name" />
      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Doctor.age" parent="Doctor">
      <preConditions />
      <postConditions>
        <condition path="Doctor.age" />
      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Doctor" parent="Episode">
      <preConditions />
      <postConditions>
        <condition path="Doctor" />
      </postConditions>
      <mulConditions>
        <condition name="EpisodeHasDoctor" rolename="episode" cardinality="0..*" />
      </mulConditions>
    </goal>
    <goal name="Prescription.desc" parent="Prescription">
      <preConditions>
        <condition path="Prescription.episode.report.desc" />
      </preConditions>
      <postConditions>
        <condition path="Prescription.desc" />
      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Prescription" parent="Episode">
