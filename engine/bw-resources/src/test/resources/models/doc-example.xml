      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Medication.quantity" parent="Medication">
      <preConditions />
      <postConditions>
        <condition path="Medication.quantity" />
      </postConditions>
      <mulConditions />
    </goal>
    <goal name="Medication.heartImpact" parent="Medication">
      <preConditions />
      <postConditions>
        <condition path="Medication.heartImpact" />
