      <mulConditions>
        <condition name="PrescriptionHasMedication" rolename="medication" cardinality="1..*" />
        <condition name="EpisodeHasPrescription" rolename="episode" cardinality="1" />
      </mulConditions>
    </goal>
    <goal name="Episode.reserveDate" parent="Episode">
